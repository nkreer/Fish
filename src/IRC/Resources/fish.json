{
  "default_nickname": "FishBot",
  "default_realname": "Fish - IRC Bot",
  "default_username": "Fish",
  "default_hostname": "Fish",
  "default_quitmsg": "Leaving",
  "command_prefix": [
    ".",
    "!",
    "\\",
    "@"
  ],
  "cpu_idle": 10,
  "authentication_message": {
    "enabled": true,
    "message": "You have been identified."
  },
  "default_ctcp_replies": {
    "VERSION": "Fish 2.0 Catfish - Developer Version <https://github.com/nkreer/Fish>"
  },
  "spam_protection": {
    "enabled": true,
    "max_commands": 10,
    "time": 60,
    "message": "You're currently blocked from using commands because you were using too many."
  },
  "invalid_permissions": "Sorry, you do not have the required permissions to use this command.",
  "disable_management": false,
  "auto_reconnect_after_timeout": 500
}